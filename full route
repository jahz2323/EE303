      analogWrite(motor2PWM, 0); // set speed of motor
      delay(500);
      active = 1;
    }
    if (_move = 5) // Position = 2
    {
      active = 1;
    }
    if (_move = 6) // Postition = 3
    {
      active = 1;
    }
    if (_move = 7) // Position = 6
    {
      active = 1;
